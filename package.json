{
  "name": "git-calendar",
  "version": "0.0.0",
  "description": "this package allows you to render yearly activities like github",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "ava",
    "npm-publish": "npm run build && npm publish --access=public"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [
    "git calendar"
  ],
  "author": "Harcop Toluwap",
  "license": "ISC",
  "devDependencies": {
    "ava": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {}
}
